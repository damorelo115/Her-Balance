    <Button fx:id="predictCycle" layoutX="50.0" layoutY="212.0"  text="Predict Next Cycle" />

    <Text fx:id="predictionText" layoutX="50.0" layoutY="288.0" text="Your next cycle will be shown here" />
    <Pane layoutX="-1.0" prefHeight="76.0" prefWidth="585.0">
        <children>
            <Label contentDisplay="CENTER" layoutX="211.0" layoutY="25.0" text="Period Tracker">
