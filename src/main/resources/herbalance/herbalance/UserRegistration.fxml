<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="471.0" prefWidth="690.0" style="-fx-background-color: FFFFFF;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="herbalance.herbalance.UserRegistrationController">
   <children>
      <Label alignment="CENTER" layoutX="205.0" layoutY="88.0" prefHeight="35.0" prefWidth="207.0" text="Already have an account?">
         <font>
            <Font name="Poppins ExtraBold" size="13.0" />
         </font>
      </Label>
      <Label fx:id="signinButton" alignment="CENTER" layoutX="432.0" layoutY="93.0" prefHeight="25.0" prefWidth="112.0" style="-fx-background-color: AFE9E5;" text="Sign In">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Label>
      <TextField fx:id="userName" layoutX="211.0" layoutY="176.0" onAction="#registration" prefHeight="25.0" prefWidth="269.0" promptText="Username" />
      <Label layoutX="211.0" layoutY="149.0" text="Username">
         <font>
            <Font name="Poppins ExtraBold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="213.0" layoutY="226.0" text="Password">
         <font>
            <Font name="Poppins ExtraBold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="registerButton" layoutX="245.0" layoutY="373.0" mnemonicParsing="false" onAction="#onButtonClick" prefHeight="25.0" prefWidth="187.0" style="-fx-background-color: AFE9E5;" text="Register">
         <font>
            <Font name="Poppins ExtraBold" size="13.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Pane prefHeight="80.0" prefWidth="690.0" style="-fx-background-color: AFE9E5;">
         <children>
            <Label alignment="CENTER" layoutX="242.0" layoutY="23.0" prefHeight="35.0" prefWidth="207.0" text="Sign Up">
               <font>
                  <Font name="Poppins ExtraBold" size="24.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Label layoutX="211.0" layoutY="304.0" text="Confirm Password">
         <font>
            <Font name="Poppins ExtraBold" size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="userPassword" layoutX="211.0" layoutY="252.0" onAction="#createpassword" prefHeight="25.0" prefWidth="269.0" promptText="Password" />
      <PasswordField fx:id="confirmPassword" layoutX="211.0" layoutY="331.0" onAction="#passwordconfirm" prefHeight="25.0" prefWidth="269.0" promptText="Confirm Password" />
      <Label fx:id="usernameLabel" layoutX="376.0" layoutY="146.0" prefHeight="27.0" prefWidth="245.0" />
      <Label fx:id="passwordLabel" layoutX="376.0" layoutY="222.0" prefHeight="27.0" prefWidth="245.0" />
      <Label fx:id="confirmLabel" layoutX="376.0" layoutY="301.0" prefHeight="27.0" prefWidth="245.0" />
      <Label fx:id="registrationResult" contentDisplay="CENTER" layoutX="90.0" layoutY="410.0" prefHeight="35.0" prefWidth="510.0">
         <font>
            <Font name="Poppins ExtraBold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
